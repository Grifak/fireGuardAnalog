   <children>
      <Label fx:id="welcomeText" prefHeight="17.0" prefWidth="120.0" text="Label" />
      <TextField fx:id="textField" alignment="TOP_RIGHT" onAction="#onHelloButtonClick" />
      <Button id="button1" mnemonicParsing="false" onAction="#onHelloButtonClick" text="Button" VBox.vgrow="NEVER" />
   </children>
</VBox>
